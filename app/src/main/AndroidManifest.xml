<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="androiddev.funnieststand_upquotes">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".subActivities.CarlinActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".subActivities.MitchActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".subActivities.StevenActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".subActivities.BillActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".subActivities.JimmyActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".subActivities.LouisActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".SplashScreen"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>